<?xml version="1.0" encoding="utf-8"?>
<!--TODO-->
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layoutDirection="rtl"
    android:background="@drawable/table_content_black_cell_bg">

    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:id="@+id/layBorderRight"
        android:orientation="vertical"
        android:background="@color/colorBlack">
    </LinearLayout>

    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:id="@+id/layBorderLeft"
        android:orientation="vertical"
        android:background="@color/colorBlack">
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:id="@+id/layBorderTop"
        android:background="@color/colorBlack"
        android:orientation="horizontal">
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100sdp"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/lblNameKala"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/table_content_black_cell_bg"
                android:gravity="center"
                android:text="تsadddddddddddثحبتحتبثبخن"
                android:textColor="@color/colorBlack"
                android:textDirection="rtl"
                style="@style/txt_print_large" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="قبهحختبتثح"
                android:id="@+id/lblShomarehBach"
                android:textColor="@color/colorBlack"
                style="@style/txt_print_large"
                android:gravity="center"
                android:background="@drawable/table_content_black_cell_bg"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/lblTedad"
                android:text="بحخصتخحصثتب"
                android:textColor="@color/colorBlack"
                style="@style/txt_print_large"
                android:gravity="center"
                android:background="@drawable/table_content_black_cell_bg"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/lblFee"
                android:text="خثبنتحخبت"
                android:textColor="@color/colorBlack"
                style="@style/txt_print_large"
                android:gravity="center"
                android:background="@drawable/table_content_black_cell_bg"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:id="@+id/layBorderBottom"
        android:background="@color/colorBlack"
        android:orientation="horizontal">
    </LinearLayout>

</LinearLayout>