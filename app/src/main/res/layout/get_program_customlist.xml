<?xml version="1.0" encoding="utf-8"?>

<com.ramotion.foldingcell.FoldingCell
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:folding-cell="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    folding-cell:additionalFlipsCount="3"
    folding-cell:animationDuration="2000"
    folding-cell:backSideColor="@color/colorBackSideFolding"
    folding-cell:cameraHeight="15"
    android:layoutDirection="rtl"
    android:clipChildren="false"
    android:clipToPadding="false">

    <!--CONTENT LAYOUT-->
    <include layout="@layout/get_program_fold_content"/>

    <!--TITLE LAYOUT-->
    <include layout="@layout/get_program_fold_title"/>


</com.ramotion.foldingcell.FoldingCell>